<UserControl    x:Class="PayrollSystem.Views.OutputComissionWorkers"    xmlns="https://github.com/avaloniaui"    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"    xmlns:viewModels="clr-namespace:PayrollSystem.ViewModels"    d:DesignHeight="450"    d:DesignWidth="800"    mc:Ignorable="d">    <StackPanel Orientation="Vertical">        <DataGrid            Name="AllWorkers"            Width="700"            Height="700"            AutoGenerateColumns="True"            Background="#FF90B2B5"            CanUserResizeColumns="True"            Items="{Binding ComissionWageWorkers}" />    </StackPanel>    <Design.DataContext>        <viewModels:AddComissionWageWorkerViewModel />    </Design.DataContext></UserControl>