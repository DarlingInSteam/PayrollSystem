<UserControl    x:Class="PayrollSystem.Views.AddComissionWageWorker"    xmlns="https://github.com/avaloniaui"    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"    xmlns:viewModels="clr-namespace:PayrollSystem.ViewModels"    d:DesignHeight="450"    d:DesignWidth="800"    mc:Ignorable="d">    <Grid>        <WrapPanel            Grid.RowSpan="2"            Width="363"            Height="244"            HorizontalAlignment="Left"            VerticalAlignment="Top"            Background="#FF90B2B5"            Orientation="Vertical">            <Label                Width="363"                Background="#FF476F8E"                Content="Добавление сотрудника с комиссионной оплатой"                FontFamily="Copperplate Gothic Bold"                FontSize="14" />            <TextBox                Name="ComissionNameBox"                Width="174"                Height="23"                Margin="0,10,0,0"                HorizontalAlignment="Center"                TextWrapping="Wrap"                Watermark="Имя" />            <ComboBox                Name="ComissionGenderComboBox"                Width="174"                Height="30"                Margin="0,10,0,0"                HorizontalAlignment="Center">                <Button                    HorizontalAlignment="Center"                    VerticalAlignment="Center"                    Content="Female"                    Foreground="Black"                    IsEnabled="False"                    RenderTransformOrigin="0.5,0.5" />                <Button                    HorizontalAlignment="Center"                    VerticalAlignment="Center"                    Content="Male"                    Foreground="Black"                    IsEnabled="False"                    RenderTransformOrigin="0.5,0.5" />            </ComboBox>            <TextBox                Name="ComissionSalaryBox"                Width="174"                Height="23"                Margin="0,10,0,0"                RenderTransformOrigin="1.35,0.559"                TextWrapping="Wrap"                Watermark="Зарплата" />            <TextBox                Name="ComissionPercentBox"                Width="174"                Height="23"                Margin="0,10,0,0"                RenderTransformOrigin="1.35,0.559"                TextWrapping="Wrap"                Watermark="Процент от продаж" />            <Button                Name="BtnAddComissionWorker"                Width="200"                Height="30"                Margin="0,10,0,0"                HorizontalAlignment="Center"                ClickMode="Press"                Content="Добавить работника" />        </WrapPanel>    </Grid>    <Design.DataContext>        <viewModels:AddComissionWageWorkerViewModel />    </Design.DataContext></UserControl>