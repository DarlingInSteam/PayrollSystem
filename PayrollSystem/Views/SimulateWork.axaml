<UserControl    x:Class="PayrollSystem.Views.SimulateWork"    xmlns="https://github.com/avaloniaui"    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"    xmlns:viewModels="clr-namespace:PayrollSystem.ViewModels"    d:DesignHeight="450"    d:DesignWidth="800"    mc:Ignorable="d">    <Grid>        <WrapPanel            Grid.RowSpan="2"            Width="363"            Height="244"            HorizontalAlignment="Right"            VerticalAlignment="Top"            Background="#FF90B2B5"            Orientation="Vertical">            <Label                Width="363"                Background="#FF476F8E"                Content="Расчет затрат"                FontFamily="Copperplate Gothic Bold"                FontSize="14" />            <Button                Margin="0,10,0,0"                VerticalAlignment="Top"                Command="{Binding SimulateWork}"                Content="Симуляция работы" />            <TextBox                Margin="0,10,0,0"                Text="{Binding Days}"                Watermark="Дни" />            <StackPanel Margin="0,10,0,0" Orientation="Horizontal">                <TextBlock Text="Расходы" />                <TextBlock Margin="170,0,0,0" Text="Отработанные дни" />            </StackPanel>            <StackPanel Margin="0,10,0,0" Orientation="Horizontal">                <TextBlock                    Width="181"                    Height="25"                    Text="{Binding Expenses, Mode=TwoWay}" />                <TextBlock                    Width="181"                    Height="25"                    Margin="170,0,0,0"                    Text="{Binding WorkedDaysCount, Mode=TwoWay}" />            </StackPanel>        </WrapPanel>    </Grid>    <Design.DataContext>        <viewModels:SimulateWorkViewModel />    </Design.DataContext></UserControl>